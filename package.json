    "moment": "^2.18.1",
    "next": "^2.1.1",
    "path-match": "^1.2.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-lifecycle-hoc": "^3.1.0",
    "recompose": "^0.23.1",
    "styled-components": "^1.4.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.6",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^4.3.0",
